 //         Where SE_DATE > '".date("Y-m-d",strtotime("now-120 hour",time()))."' 
  echo '<br>'.$lotid ;
  echo '<H2>Titan AR Data</H2>' ;

  $sql = "Select [DEV_NO],convert(char(19),SE_DATE,120) [SE_DATE],convert(char(19),MS_DATE,120)[MS_DATE],convert(char(19),ME_DATE,120) [ME_DATE]
            ,[Glass_ID],[Lot_ID],[Slot_No],[OP_NO],[Recipe_ID],[measure_cnt],[csv_floder],convert(char(19),titan_update_time,120) TITAN_UPD , PH_STEP, PH_EQ
            ,convert(char(19),LAST_PH_TIME,120) LAST_PH_TIME
          FROM [ArrayPH].[dbo].[AR_Titan] 

          ";

          

  if ($lotid <> '' ) 
  {     
    if (strlen($lotid)<> 10){ //指定前n碼,要限定時間
      $get_date1 = date("Y-m-d H:i:s",strtotime("-6 day",strtotime($et)));
      $start_date1 = substr($get_date1,0,10);
    }
    $sql = $sql."   where [se_date] > '".$start_date1."' 
          and [se_date] < '".$et."'" ;
    $sql = $sql."     and substring(Glass_ID,1,len('".$lotid."')) ='".$lotid."' " ;
  }   
  elseif ( $Recipe <> '' )
  {
    //指定,要限定時間
    $get_date1 = date("Y-m-d H:i:s",strtotime("-6 day",strtotime($et)));
    $start_date1 = substr($get_date1,0,10);
    
    $sql = $sql."   where [se_date] > '".$start_date1." 00:00' 
            and [se_date] < '".$et."'" ;     
    $sql = $sql." and substring(Recipe_ID,1,len('".$Recipe."')) ='".$Recipe."' " ; 
  }else{
    $sql = $sql."   where [se_date] > '".$st."' 
          and [se_date] < '".$et."' " ;
  }

  $sql .= "Order by convert(char(19),SE_DATE,120),convert(char(19),ME_DATE,120) ,[Lot_ID],[Slot_No] ";

  $result = sqlsrv_query($mssql_conn, $sql);
  echo '<BR>'.$sql.'<BR>';
  $i =0 ;
  $Data=array();
  while ($row = sqlsrv_fetch_array($result, SQLSRV_FETCH_ASSOC)) {
    $Data[$i] = array() ;
    $Data[$i][0] = trim($row['Lot_ID']) ;
    $Data[$i][1] = trim($row['Slot_No']) ;
    $Data[$i][2] = trim($row['Glass_ID']) ;
    $Data[$i][3] = trim($row['OP_NO']) ;
    $Data[$i][4] = trim($row['DEV_NO']) ;
    $Data[$i][5] = trim($row['Recipe_ID']) ;
    //$Data[$i][13] = trim($row['SE_DATE']) ;
    //$Data[$i][7] = substr(trim($row['MS_DATE']),0,16) ;
    $Data[$i][6] = substr(trim($row['ME_DATE']),0,16) ;
    $Data[$i][7] = $row['measure_cnt'] ;
    $Data[$i][8] = substr(trim($row['LAST_PH_TIME']),0,16) ;
    $Data[$i][9] = trim($row['PH_STEP']) ;
    $Data[$i][10] = trim($row['PH_EQ']) ;
    $Data[$i][11] = trim($row['csv_floder']) ;
    $Data[$i][12] = substr(trim($row['TITAN_UPD']),0,16) ;
    $i++;
  }    
  //$itt = array('Lot_ID','Slot_No','Glass_ID','OP_NO','DEV_NO','Recipe_ID','Titan_Time','Start_Time','End_Time','Last_PH_TIME','PH_STEP','PH_EQ','CSV File') ;
  $itt = array('Lot_ID','Slot_No','Glass_ID','OP_NO','DEV_NO','Recipe_ID','Titan_Time','measure_cnt','Last_PH_TIME','PH_STEP','PH_EQ','CSV File','TITAN_UPD') ;


  echo "<span style='font-size: 12px;'><br>" ;
//goto QQB;
  echo '<table  style="border:3px #cccccc solid;" cellpadding="2" border=1>';
  echo '<tr>';
  for ($i=0 ; $i < count($itt) ; $i++){
    echo "<td>".$itt[$i]."</td>";
  }   
  echo '</tr> ';
  for ($i= 0 ; $i < count($Data) ; $i++){
    $temp_e_time = str_replace(' ', '%2B', $Data[$i][6]);  #$row['SE_DATE']
    echo '<tr>';
    for ($j = 0;$j<count($Data[1]);$j++){
      if ( $itt[$j] == 'Glass_ID' ){
