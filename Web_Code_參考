                    " target=_blank><div style ='text-align:left;'><span style='font-size: 14px;'>".$Data[$i][$j]."</a></td>";        
      }elseif( substr($Data[$i][$j],0,4)=='http' ){
        echo "<td><a href=".$Data[$i][$j].">csv</a></td>";                
      }else{
        echo "<td>".$Data[$i][$j]."</td>" ;
      }
    }
    echo '</tr> ';
  }
  echo '</table  >';
QQB:




?>



