  session_start();

  date_default_timezone_set('Asia/Taipei');

  set_time_limit(0);
  error_reporting(E_ALL^E_NOTICE^E_WARNING);


  $st = $_GET[st];
  $et = $_GET[et];
  $lotid = $_GET[lotid];
  $Recipe = $_GET[Recipe];


  $st = substr($st,0,4).'-'.substr($st,5,2).'-'.substr($st,8,2).' '.substr($st,11,2).':'.substr($st,14,2);
  $et = substr($et,0,4).'-'.substr($et,5,2).'-'.substr($et,8,2).' '.substr($et,11,2).':'.substr($et,14,2);

  echo '<form action="AR_Titan_Data_V2.php" method=GET >';
  //echo '<form action="AR_Titan_Data_V2.php" method=GET target="top" >';
  //if ($start_date == '' or $start_date =='--' ) { $start_date =date("m/d/Y"); }
  //echo 'Start Date: </span>';
  //echo '<input type="text" id = "start_date" name="start_date" value = '.$get_date.' ><BR>';

  if ($et == '' or $et =='--' or trim($et) =='-- :' ) { 
    //$et = date('Y-m-d H:i:s', time()); // 現在的日期和時間
    $et = date('Y-m-d H:i:s', strtotime('+0 hours')); // 現在的日期和時間
    $st = date('Y-m-d H:i:s', strtotime('-8 hours')); // 現在減去2天的日期和時間
  }

  if (strtotime('+3 hours') >= strtotime($et) ){$et = date("Y-m-d H:i:s",strtotime("+2 hour",strtotime($et))) ;}

  //echo '<br>'.$st ;
  //echo '<br>'.$et.'<br>' ;
  echo 'Start Date: </span>';
  echo '<input type="datetime-local" name="st" value="'.$st.'"> ';
    echo '  </span></span> ';
    echo 'End Date: </span> ';
    echo '<input type="datetime-local" name="et" value="'.$et.'"> ';

  echo '<br><span>LOT_ID:  </span><input type="text" id = "lotid" name="lotid" > 可輸前n碼<BR>';
  echo '<span>Recipe : </span><input type="text" id = "Recipe" name="Recipe" > ';
  echo '<BR>';
  echo "  <input type=submit value=Submit></form>";

  $serverName = "10.30.163.208";
  $connectionOptions = array(
    "UID" => "sa",
    "PWD" => "Auol6bi1",
    "CharacterSet" => "UTF-8"
  );
  $mssql_conn = sqlsrv_connect($serverName, $connectionOptions);

  if (!$mssql_conn) {
    die("資料庫連線失敗: " . print_r(sqlsrv_errors(), true));
  }

  //$Sheet_id ='FV24G7SY70P15' ;
