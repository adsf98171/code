  $serverName = 10.30.163.208;
  $connectionOptions = array(
    UID = sa,
    PWD = Auol6bi1,
    CharacterSet = UTF-8
  );
  $mssql_conn = sqlsrv_connect($serverName, $connectionOptions);

  if (!$mssql_conn) {
    die(資料庫連線失敗  . print_r(sqlsrv_errors(), true));
  }

  $DDD = date('Y-m-d His', time());
  $Sheet_id ='FV24G7SY70P15' ;
  $Recipe_ID = '8103' ;

  $sql = Select  [Machine_ID],convert(char(19),Test_Time,120) Test_Time,[Lot_ID],[Recipe_ID],[Process_Name]
            ,[PPID],[Recipe_Name],[Slot_No],[Glass_ID],[Data_Number]
            ,[Point_No],[X_R],[Y_R],[PointJudge],[SubRecipeNo]
            ,[L1],[L2],[L3],[L4],[L5],[A1],[A2]
            ,[Point_Chip],[Line_Name],[Scan],[Lens],[SPEC],special_type,Img1,Img2 
          FROM [ArrayPH].[dbo].[AR_Measure]
          Where Glass_ID = '.$Sheet_id.' and Recipe_ID = '.$Recipe_ID.' 
            and Test_Time between '.date(Y-m-d His,strtotime(-10 minute,strtotime($ttdate))).' and '.date(Y-m-d His,strtotime(+10 minute,strtotime($ttdate))).' 

          Order by substring(Point_Chip,2,1),Point_No
          ;
  
            and substring([Lens],1,1) ='M' and special_type = ''
  $result = sqlsrv_query($mssql_conn, $sql);
