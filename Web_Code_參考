    if ($ii == 2 ){
      for( $k = 0 ; $k < count($data_draw_name[$j]) ;$k++){
        if ( $data_draw_name[$j][$k][1] == 1 ){$data_draw_name[$j][$k][1] = '左' ; }
        if ( $data_draw_name[$j][$k][1] == 2 ){$data_draw_name[$j][$k][1] = '右' ; }
      }      
    }
    if ($ii ==3 ){
      for( $k = 0 ; $k < count($data_draw_name[$j]) ;$k++){
        if ( $data_draw_name[$j][$k][1] == 1 ){$data_draw_name[$j][$k][1] = '左' ; }
        if ( $data_draw_name[$j][$k][1] == 2 ){$data_draw_name[$j][$k][1] = '中' ; }
        if ( $data_draw_name[$j][$k][1] == 3 ){$data_draw_name[$j][$k][1] = '右' ; }
      }      
    }           
    if ($ii ==4 ){
      for( $k = 0 ; $k < count($data_draw_name[$j]) ;$k++){
        if ( $data_draw_name[$j][$k][1] == 1 ){$data_draw_name[$j][$k][1] = '左外' ; }
        if ( $data_draw_name[$j][$k][1] == 2 ){$data_draw_name[$j][$k][1] = '左內' ; }
        if ( $data_draw_name[$j][$k][1] == 3 ){$data_draw_name[$j][$k][1] = '右內' ; }
        if ( $data_draw_name[$j][$k][1] == 4 ){$data_draw_name[$j][$k][1] = '右外' ; }
      }      
    }   
    if ($ii ==5 ){
      for( $k = 0 ; $k < count($data_draw_name[$j]) ;$k++){
        if ( $data_draw_name[$j][$k][1] == 1 ){$data_draw_name[$j][$k][1] = '左' ; }
        if ( $data_draw_name[$j][$k][1] == 2 ){$data_draw_name[$j][$k][1] = '左中' ; }
        if ( $data_draw_name[$j][$k][1] == 3 ){$data_draw_name[$j][$k][1] = '中' ; }
        if ( $data_draw_name[$j][$k][1] == 4 ){$data_draw_name[$j][$k][1] = '右中' ; }
        if ( $data_draw_name[$j][$k][1] == 5 ){$data_draw_name[$j][$k][1] = '右' ; }
      }      
    }      
  }




  //echo '<br>'.(json_encode($data_draw_name[$j])) ;

}
echo '<br>'.(json_encode($data_draw_name)) ;



##畫Line chart
$tt = 0 ;
$data_draw_CD = array() ;
$j = 0 ;
$data_draw_CD[$j] = array() ;
for ($j =0 ; $j < count($item_name) ;$j++ ){
  $data_draw_CD[$j][0] = array() ;
  for ($i= 0 ; $i < count($Data) ; $i++){
    if ( $Data[$i][29] <>'CenterCD' ){
      for ( $s = 0 ;$s < count($data_draw_name[$j]) ;$s++ ){
        if ( ($data_draw_name[$j][$s][0] > (float)$Data[($i)][11]-7.6) and ($data_draw_name[$j][$s][0] < (float)$Data[($i)][11]+7.6)  ){

          ##65" 接光罩才須要跨scan比較,其他多一個空格
          $m = count($data_draw_CD[$j][$s]) ;
          if (( substr($Sheet_id,0,1)<>'F' and substr($data_draw_CD[$j][$s][($m-1)][0],0,5) <> $Data[$i][26] )
                or ( substr($Sheet_id,0,1) == 'F' and $Data[($i+1)][24] <> $Data[$i][24] )   ){ 
            $data_draw_CD[$j][$s][$m] = array() ; 
            $data_draw_CD[$j][$s][$m][0] = ''  ;  
            $data_draw_CD[$j][$s][$m][1] = null  ;  
          }

          ##抓Y軸資料-->依X軸分開畫圖
          $m = count($data_draw_CD[$j][$s]) ;
          $data_draw_CD[$j][$s][$m] = array() ; 
          //$data_draw_CD[$j][$s][$m][0] = (float)$Data[$i][12]  ;
          $data_draw_CD[$j][$s][$m][0] = $Data[$i][26].'<br>'.$Data[$i][27].'_'.$tt  ;  
          if ( (float)$Data[$i][(15+$j)] <>'' ) { $data_draw_CD[$j][$s][$m][1] = (float)$Data[$i][(15+$j)]  ;} else{ $data_draw_CD[$j][$s][$m][1]= null ;}
          //echo '<br>aaa'.$i.'-'.$s.'-'.$data_draw_name[$j][$s][0].'-'.(round(((float)$Data[($i)][11]/10),0)*10).'==='.(float)$Data[$i][(15+$j)].'--'.$data_draw_CD[$j][$s][$m][0] ;
          //echo '<br>'.(json_encode($data_draw_CD[0])) ;
          goto ESS ;
        }
        //echo '<br>'.$i.'-'.$s.'-'.$data_draw_name[$j][$s][0].'-'.(round(((float)$Data[($i)][11]/10),0)*10).'==='.(float)$Data[$i][(15+$j)].'-'.((float)$Data[($i)][11]-5).'+'.((float)$Data[($i)][11]+5) ;
      }
      $data_draw_CD[$j][$s] = array() ; 
ESS:
    }
    if ( $Data[$i][26] <> $Data[($i+1)][26] or $Data[$i][27] <> $Data[($i+1)][27] ){
      $tt = 0 ;
    }else{
      $tt++;
    }
  }
}
//echo '<br>'.$data_draw_name[0][5][0].'-'.(json_encode($data_draw_CD[0][4])) ;
//echo '<br>'.$data_draw_name[0][5][0].'-'.(json_encode($data_draw_CD[0][5])) ;

###DCD chart
$data_draw_DiffCD = array() ;
$j = 0 ;
$data_draw_DiffCD[$j] = array() ;
for ($j =0 ; $j < count($item_name) ;$j++ ){
  //echo '<br>AAA='.$item_name[$j].'<br>' ;
  $data_draw_DiffCD[$j][0] = array() ;
  for ($i= 0 ; $i < count($Data) ; $i++){
    if ( $Data[$i][29] <>'CenterCD' and (float)$Data[$i][(15+$j)] <>'' and (float)$Data[($i+1)][(15+$j)] <>'' ){

      if ( ( substr($Sheet_id,0,1) <> 'F' and $Data[$i][26] == $Data[($i+1)][26] and ((float)substr($Data[($i+1)][27],-1) - (float)substr($Data[$i][27],-1)) == 1 ) 
          or ( substr($Sheet_id,0,1) == 'F' and $Data[$i][27] <> $Data[($i+1)][27] )    ){

        for ( $s = 0 ;$s < count($data_draw_name[$j]) ;$s++ ){
          //if ( $data_draw_name[$j][$s][0] == round(((float)$Data[($i)][11]/10),0)*10 and $data_draw_name[$j][$s][0] == round(((float)$Data[($i+1)][11]/10),0)*10  ){
          if (      ($data_draw_name[$j][$s][0] > (float)$Data[($i)][11]-7.6) and ($data_draw_name[$j][$s][0] < (float)$Data[($i)][11]+7.6) 
               and  ($data_draw_name[$j][$s][0] > (float)$Data[($i+1)][11]-7.6) and ($data_draw_name[$j][$s][0] < (float)$Data[($i+1)][11]+7.6)  ){
            //echo '<br>'.$i.'-'.$m.'-'.substr($data_draw_DiffCD[$j][$s][($m-1)][0],0,5).'-'.$Data[$i][26] ;
            ##65" 接光罩才須要跨scan比較,其他多一個空格
            $m = count($data_draw_DiffCD[$j][$s]) ;
            if (( $m >0 and substr($Sheet_id,0,1) <>'F' and substr($data_draw_DiffCD[$j][$s][($m-1)][0],0,5) <> $Data[$i][26] ) 
                  or ( $m >0 and substr($Sheet_id,0,1) =='F' and $Data[($i+1)][24] <> $Data[$i][24] )  ){ 
              $data_draw_DiffCD[$j][$s][$m] = array() ; 
              $data_draw_DiffCD[$j][$s][$m][0] = ''  ;  
              $data_draw_DiffCD[$j][$s][$m][1] = null  ;  
            }

            ##抓Y軸資料-->依X軸分開畫圖
            $m = count($data_draw_DiffCD[$j][$s]) ;
           
            //$data_draw_DiffCD[$j][$s][$m][0] = (float)$Data[$i][12]  ;   
            $data_draw_DiffCD[$j][$s][$m][0] = $Data[$i][26].'<br>'.$Data[$i][27].'/'.$Data[($i+1)][27]  ;     
            $data_draw_DiffCD[$j][$s][$m][1] = round((float)$Data[($i)][(15+$j)] - (float)$Data[($i+1)][(15+$j)],3)  ;
            //echo '<br>-'.$i.'-'.$m.'-'.$data_draw_name[$j][$s][0].'-'.$Data[($i)][11].'-'.$Data[($i)][12] ;
            $i++ ;
            goto ESF ;
          }
        }
        $data_draw_DiffCD[$j][$s] = array() ; 
      ESF:
      }
    }
  }

}
//echo '<br>'.(json_encode($data_draw_DiffCD[1][7])) ;
