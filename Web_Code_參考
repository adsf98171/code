  echo '  document.getElementById("myChart_0a").style.display = "block";  ';
  echo '  document.getElementById("myChart_0b").style.display = "block";  ';
  echo '  document.getElementById("myChart_0c").style.display = "block";  ';
  
  // 顯示指定的圖表
  echo 'function showChart(chartId) {   ';
  for ($j = 0 ; $j < count($item_name); $j++ ){
    // 先隱藏所有的圖表
    echo 'document.getElementById("myChart_'.$j.'").style.display = "none";   ';
    echo 'document.getElementById("myChart_'.$j.'a").style.display = "none";   ';
    echo 'document.getElementById("myChart_'.$j.'b").style.display = "none";   ';
    echo 'document.getElementById("myChart_'.$j.'c").style.display = "none";   ';
  }
    // 顯示指定的圖表
  echo '  document.getElementById(chartId).style.display = "block";  ';
  echo '  document.getElementById(chartId+"a").style.display = "block";  ';
  echo '  document.getElementById(chartId+"b").style.display = "block";  ';
  echo '  document.getElementById(chartId+"c").style.display = "block";  ';
  echo '}    ';

echo '</script> ';


