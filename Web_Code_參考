    {
      $AR_Hist[$record_count][0] = $c->LOT_ID ;
      $AR_Hist[$record_count][1] = $c->SHEET_ID ;
      $AR_Hist[$record_count][2] = $c->DEV_KIND ;
      $AR_Hist[$record_count][3] = $c->OP_NO ;
