function createChart($viewer, $j,$title,$item_name,$series)
{

    $max_value = $series[0][2] ;
    $min_value = $series[0][2] ;
    $dataX = array() ;
    $dataY = array() ;
    $i = 0 ;
    foreach ($series as $item) {
      if ($item[2] == null ){
        //$dataX[$i] = (float)$item[0];
        //$dataY[$i] = (float)$item[1];
        //$dataZ[$i] =  null ; 
        //echo '<br>miss point :X = '.(float)$item[0].'-Y = '.(float)$item[1] ;
      }else{
              $dataX[$i] = (float)$item[0];
      $dataY[$i] = (float)$item[1];
        $dataZ[$i] = (float)$item[2]; 
        if ( (float)$item[2] > $max_value ) { $max_value = (float)$item[2] ; }
        if ( (float)$item[2] < $min_value ) { $min_value = (float)$item[2] ; }
        $i++ ;
      }


    } 

    $inte = ($max_value - $min_value)/8 ;

    //echo '<br>'; print_r($dataX) ;
    //echo '<br>'.count($dataY) ;

    # The x and y coordinates of the grid
    //$dataX = array(-4, -3, -2, -1, 0, 1, 2, 3, 4);
    //$dataY = array(-4, -3, -2, -1, 0, 1, 2, 3, 4);

    # Use random numbers for the z values on the XY grid
    //$r = new RanSeries(99);
    //$dataZ = $r->get2DSeries(count($dataX), count($dataY), -0.9, 0.9);

    # Create a XYChart object of size 420 x 360 pixels
    $c = new XYChart(780, 560);

    # Set the plotarea at (30, 25) and of size 300 x 300 pixels. Use semi-transparent grey
    # (0xdd000000) horizontal and vertical grid lines
    $c->setPlotArea(30, 65, 680, 450, -1, -1, -1, 0xdd000000, -1);
    $c->addTitle("Line Chart_".$title, "Arial Bold", 14, 0xffffff)->setBackground(
    $c->patternColor(array(0x004000, 0x008000), 2));
    # Set the x-axis and y-axis scale
    $c->xAxis->setLinearScale(0, 1850, 200);
    $c->yAxis->setLinearScale(0, 1500, 200);

    # Add a contour layer using the given data
    $layer = $c->addContourLayer($dataX, $dataY, $dataZ);

    # Enable contour label by setting its style to black (000000) 8pt default bold font
    $layer->setContourLabelStyle("bold", 8, 0x000000);

    # Set the label to display the value in a CDML block with semi-transparent white (4FFFFFFF)
    # background and 2 pixels left/right margin and 1 pixel top/bottom margin
    $layer->setContourLabelFormat("<*block,bgcolor=4FFFFFFF,margin=2 2 1 1*>{value}");

    # Move the grid lines in front of the contour layer
    $c->getPlotArea()->moveGridBefore($layer);

    # Add a color axis (the legend) in which the top left corner is anchored at (350, 25). Set the
    # length to 400 300 and the labels on the right side.
    $cAxis = $layer->setColorAxis(720, 75, TopLeft, 440, Right);

    # Specify the color scale to use in the color axis
    $colorScale = array( round(($min_value*0.99),2), 0x1a9850, round(($min_value+$inte*1),2), 0x66bd63, round(($min_value+$inte*2),2), 
        0xa6d96a, round(($min_value+$inte*3),2), 0xd9ef8b, round(($min_value+$inte*4),2),
        0xfee08b, round(($min_value+$inte*5),2), 0xfdae61, round(($min_value+$inte*6),2), 0xf46d43, round(($min_value+$inte*7),2), 0xd73027, round(($max_value*1.01),2) );
    $cAxis->setColorScale($colorScale);

    # Output the chart
    $viewer->setChart($c, SVG);

    # Include tool tip for the chart
    $viewer->setImageMap($c->getHTMLImageMap("", "",
                          "title='<*cdml*><*b*>X: {x|2}<*br*>Y: {y|2}<*br*>Z: {z|2}'"));

    # Output Javascript chart model to support contour chart tooltips
    $viewer->setChartModel($c->getJsChartModel());
}



function draw_heatmap($j,$title,$item_name,$series){

  //echo '<br>'.json_encode($series) ;
  #圖表   
  echo '<figure class="highcharts-figure" style="text-align: left;">
          <div id="myChart_'.$j.'" style="height: 600px; width: 800px"></div>
        </figure> ';

  echo '<script type="text/javascript">'  ;
  echo 'Highcharts.chart("myChart_'.$j.'", {
          chart: {
              type: "heatmap",
              plotBorderWidth: 1
          },
          title: {
              text: "Heatmap_'.$title.'"
          },
          xAxis: {
              title: {
                  text: ""
              }
          },
          yAxis: {
              title: {
                  text: ""
              }
          },       
          colorAxis: {
              stops: [
                  [0, "#3060cf"],
                  [0.5, "#fffbbc"],
                  [1, "#c4463a"]
              ],
              min: 3.4,
              max: 3.9,
              labels: {
                  style: {
                      color: "black"
                  }
              }
          },

          series: [{
              borderWidth: 1,
              data: '.json_encode($series).',
              dataLabels: {
                  enabled: true,
                  color: "#000000"
              },
              color: "blue" ,
              interpolation: true
          }],
          responsive: {
            rules: [{
              condition: {
                minWidth: 800
              },
              chartOptions: {
                chart: {
                  height: 600
                }
              }
            }]
          }
      }); ';

  echo '</script> ';

}


function draw_Line($j,$title,$item_name,$series,$data_draw_CD_name){

  //echo '  <meta http-equiv="Content-Type" content="text/html; charset=utf8"> ';
  //echo '  <script src="../../chart.js/chart.min.js"></script>  ';
  #選項按鈕
  //echo ' <div>   ';
  //$kk= "'myChart_a".$j."'" ;
  //echo '   <input type="radio" name="chartSelection2" value="myChart_a'.$j.'" onclick="showChart1('.$kk.')"> '.$item_name[$j]  ;
  //echo ' </div>  ';
  //echo '<BR>'.json_encode($series[4]) ;
  #圖表   
  echo '<figure class="highcharts-figure" style="text-align: left;">
          <div id="myChart_'.$j.'" style="height: 600px; width: 900px"></div>
        </figure> ';

  echo '<script type="text/javascript">'  ;
  echo 'Highcharts.chart("myChart_'.$j.'", {
          chart: {
            type: "line",
            zoomType: "xy"
          },
          title: {
            text: "Line Plot of '.$title.'"
          },
          xAxis: {
            title: {
              text: ""
            },type: "category"
          },
          yAxis: {
            title: {
              text: ""
            }
          },
          series: [ ';
  for ( $k = 0 ; $k < count($series) ; $k++){
    echo '{
            name: "'.$data_draw_CD_name[$k][1].'",
            data: 
                   '.json_encode($series[$k]).'
                  
          }, ';
  }
  echo '  ]
        }); 
  
        </script> ';

}


